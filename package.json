{
  "name": "@illinois-toolkit/ilw-page",
  "description": "Illinois Toolkit: This component is used to manage major areas inside an HTML page.",
  "repository": "github:web-illinois/ilw-page",
  "private": false,
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "builder",
    "dist"
  ],
  "main": "./dist/ilw-page.cjs",
  "module": "./dist/ilw-page.js",
  "exports": {
    ".": {
      "import": "./dist/ilw-page.js",
      "require": "./dist/ilw-page.cjs"
    },
    "./*.css": "./dist/*.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.build.config.js --emptyOutDir",
    "preview": "vite preview",
    "prepack": "npm run build"
  },
  "dependencies": {
    "lit": "3.1.3"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.3.0"
  }
}
